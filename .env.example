# Server Configuration
PORT=3000
NODE_ENV=development

# API Configuration
API_PREFIX=/api/v1

# Swagger Configuration
SWAGGER_ENABLED=true
SWAGGER_PATH=/api/docs

# CORS Configuration
CORS_ORIGIN=*
CORS_CREDENTIALS=false
# Database host
DB_HOST=localhost
# Database port
DB_PORT=5432
# Database name
DB_NAME=myapp
# Database user
DB_USER=postgres
# Database password
DB_PASSWORD=
# JWT secret key
JWT_SECRET=your-secret-key-change-in-production
